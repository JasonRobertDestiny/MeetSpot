# MeetSpot Environment Variables
# Copy this file to .env and fill in your actual values

# Required: Amap (高德地图) API credentials
AMAP_API_KEY=your_amap_api_key_here
AMAP_SECURITY_JS_CODE=your_amap_security_js_code_here

# Optional: Port configuration (defaults to 8000)
PORT=8000

# Optional: OpenAI API (if using AI features)
# OPENAI_API_KEY=sk-your-openai-api-key
# OPENAI_BASE_URL=https://api.openai.com/v1

# Optional: Log level
LOG_LEVEL=info
