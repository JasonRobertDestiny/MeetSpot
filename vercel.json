{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/health",
      "dest": "/api/index.py"
    },
    {
      "src": "/config",
      "dest": "/api/index.py"
    },
    {
      "src": "/workspace/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/public/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "AMAP_API_KEY": "@amap_api_key",
    "AMAP_SECURITY_JS_CODE": "@amap_security_js_code"
  }
}
